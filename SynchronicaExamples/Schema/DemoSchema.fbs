namespace Synchronica.Examples.Schema;

enum ClientMessageIds : int {
	LoginRequest = 1,
	InputRequest = 2,
}

enum ServerMessageIds : int {
	LoginResponse = 1,
	InputResponse = 2,
	SynchronicaData = 3,
}

enum Command : int {
	Up = 0,
	Down = 1,
	Left = 2,
	Right = 3,
}

table LoginRequest {
	name: string;
}

table LoginResponse {
	objectId: int;
}

table InputRequest {
	milliseconds: int;
	command: Command;
}
